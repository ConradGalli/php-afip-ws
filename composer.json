{
    "name": "reyesoft/php-afip-ws",
    "description": "Librería PHP para interacturar con el webservice de AFIP (Argentina -> factura electrónica).",
    "type": "library",
    "keywords": ["afip", "argentina", "factura"],
    "homepage": "https://github.com/reyesoft/php-afip-ws",
    "license": "GPL3",
    "authors": [
        {
            "name": "Ayelen Guerra"
        },
        {
            "name": "Pablo Reyes",
            "email": "pablo@reyesoft.com"
        }
    ],
    "support": {
        "issues": "https://github.com/reyesoft/php-afip-ws/issues"
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "psr-4": {
            "Reyesoft\\Afip\\": "src/Reyesoft/Afip/"
            },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "Reyesoft\\AfipTest\\": "tests"
        }
    }
}
